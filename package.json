    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "commander": "^12.0.0",
    "dockerode": "^4.0.2",
    "dotenv": "^16.4.5",
    "inquirer": "^9.2.19",
    "node-fetch": "^2.7.0",
